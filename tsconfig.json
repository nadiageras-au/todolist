{
  "compilerOptions": {
    //"target": "es6",
//    "lib": ["es5", "es6", "dom"],
    "lib": [
//      "es5",
      "es6",
      "DOM",
      "DOM.Iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src_2"
  ]
}
